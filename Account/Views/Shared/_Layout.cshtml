
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.PageTitle</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />

    <link rel="icon" type="image/x-icon" href="~/favicon.ico">
    <link rel="apple-touch-icon" href="~/icon.png">


    <!-- Bootstrap -->

    <link href='@Url.Content("~/css/bootstrap.min.css")' rel="stylesheet" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if (lt IE 9)]>
        <script src='@Url.Content("~/css/html5shiv.min.js")'></script>
        <script src='@Url.Content("~/css/respond.min.js")'></script>
    <![endif]-->
    <link href='@Url.Content("~/css/animate.css")' rel="stylesheet" />
    <link href='@Url.Content("~/css/headerfooter.css")' rel="stylesheet" />
    <link href='@Url.Content("~/css/navigation.css")' rel="stylesheet" />
    <link href='@Url.Content("~/css/custom.css")' rel="stylesheet" />
    <!--[if !IE]> -->
    <script src='@Url.Content("~/js/jquery.min.js")'></script>
    <!-- <![endif]-->
    <!--[if IE]>
        <script src="@Url.Content("~/js/jquery1.min.js")"></script>
    <![endif]-->
    <script src='@Url.Content("~/js/bootstrap.min.js")'></script>
    <script src='@Url.Content("~/js/modernizr.js")'></script>
    <script src='@Url.Content("~/js/handlebars.js")'></script>
    <script src='@Url.Content("~/js/scripts.js")'></script>
    <script src='@Url.Content("~/js/jquery.meanmenu.js")'></script>
    <script src='@Url.Content("~/js/jquery.pwstrength.js")'></script>
    <script src='@Url.Content("~/js/jquery-ui.min.js")'></script>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Roboto+Condensed&family=Roboto:wght@300;400;500&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="@Url.Content("~/css/normalize.css")">
    <link rel="stylesheet" href="@Url.Content("~/css/main.css")">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@@splidejs/splide@1.3.3/dist/css/splide.min.css">

</head>
<body class="scrollable">

    <div id='ajax_loader' style="display: none;">
        <img src='@Url.Content("~/images/Loading.gif")' />
    </div>

    @Html.Partial("_Header", null)

    <div class="container" id="container">

        <div class="row">
            @RenderSection("featured", required: false)
            <div class="navbar col-sm-3 col-xs-12">
                @Html.Partial("_SideCategories", null)
            </div>
            <div class="col-sm-9 col-xs-12">
                <div class="row">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <div style="clear: both"></div>


    @Html.Partial("_Footer", null)


    @if (System.Configuration.ConfigurationManager.AppSettings["jshelpers"] == "true")
    {

        if (HttpContext.Current.Session["users"] != null && ((List<SL.Business.UserInfo>)HttpContext.Current.Session["users"]) != null)
        {
            Html.Raw("<div class='row panel panel-default'>");
            List<SL.Business.UserInfo> users = (List<SL.Business.UserInfo>)HttpContext.Current.Session["users"];
            Html.Raw("</div");
        }
    }


    <script src="@Url.Content("~/js/vendor/modernizr-3.11.2.min.js")"></script>
    <script src="@Url.Content("~/js/plugins.js")"></script>
    <script src="@Url.Content("~/js/main.js")"></script>


</body>
    </html>

